CREATE TABLE IF NOT EXISTS "migrations" ("id" integer primary key autoincrement not null, "migration" varchar not null, "extension" varchar);
CREATE TABLE IF NOT EXISTS "groups" ("id" integer primary key autoincrement not null, "name_singular" varchar not null, "name_plural" varchar not null, "color" varchar, "icon" varchar, "is_hidden" tinyint(1) not null default '0', "created_at" datetime, "updated_at" datetime);
CREATE TABLE IF NOT EXISTS "group_permission" ("group_id" integer not null, "permission" varchar not null, "created_at" datetime, primary key ("group_id", "permission"));
CREATE TABLE IF NOT EXISTS "group_user" ("user_id" integer not null, "group_id" integer not null, "created_at" datetime, primary key ("user_id", "group_id"));
CREATE TABLE settings ("key" VARCHAR(255) NOT NULL, value CLOB DEFAULT NULL, PRIMARY KEY("key"));
CREATE TABLE IF NOT EXISTS "api_keys" ("key" varchar not null, "id" integer primary key autoincrement not null, "allowed_ips" varchar, "scopes" varchar, "user_id" integer, "created_at" datetime not null, "last_activity_at" datetime, foreign key("user_id") references "users"("id") on delete cascade);
CREATE UNIQUE INDEX "api_keys_key_unique" on "api_keys" ("key");
CREATE TABLE discussion_user (user_id INTEGER NOT NULL, discussion_id INTEGER NOT NULL, last_read_at DATETIME DEFAULT NULL, last_read_post_number INTEGER DEFAULT NULL, "subscription" varchar check ("subscription" in ('follow', 'ignore')), PRIMARY KEY(user_id, discussion_id));
CREATE TABLE email_tokens (token VARCHAR(255) NOT NULL, email VARCHAR(255) NOT NULL COLLATE "BINARY", user_id INTEGER NOT NULL, created_at DATETIME NOT NULL, PRIMARY KEY(token));
CREATE TABLE notifications (id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, user_id INTEGER NOT NULL, from_user_id INTEGER DEFAULT NULL, type VARCHAR(255) NOT NULL COLLATE "BINARY", subject_id INTEGER DEFAULT NULL, data BLOB DEFAULT NULL, created_at DATETIME NOT NULL, is_deleted BOOLEAN DEFAULT 0 NOT NULL, read_at DATETIME DEFAULT NULL);
CREATE TABLE password_tokens (token VARCHAR(255) NOT NULL, user_id INTEGER NOT NULL, created_at DATETIME NOT NULL, PRIMARY KEY(token));
CREATE TABLE IF NOT EXISTS "post_user" ("post_id" integer not null, "user_id" integer not null, foreign key("post_id") references "posts"("id") on delete cascade, foreign key("user_id") references "users"("id") on delete cascade, primary key ("post_id", "user_id"));
CREATE INDEX "notifications_user_id_index" on "notifications" ("user_id");
CREATE TABLE registration_tokens (token VARCHAR(255) NOT NULL, payload CLOB DEFAULT NULL, created_at DATETIME NOT NULL, "provider" varchar not null, "identifier" varchar not null, "user_attributes" text, PRIMARY KEY(token));
CREATE TABLE IF NOT EXISTS "login_providers" ("id" integer primary key autoincrement not null, "user_id" integer not null, "provider" varchar not null, "identifier" varchar not null, "created_at" datetime, "last_login_at" datetime, foreign key("user_id") references "users"("id") on delete cascade);
CREATE UNIQUE INDEX "login_providers_provider_identifier_unique" on "login_providers" ("provider", "identifier");
CREATE TABLE IF NOT EXISTS "access_tokens" ("id" integer primary key autoincrement not null, "token" varchar not null, "user_id" integer not null, "last_activity_at" datetime, "created_at" datetime not null, "type" varchar, "title" varchar, "last_ip_address" varchar, "last_user_agent" varchar, foreign key("user_id") references "users"("id") on delete cascade);
CREATE INDEX "access_tokens_type_index" on "access_tokens" ("type");
CREATE UNIQUE INDEX "access_tokens_token_unique" on "access_tokens" ("token");
CREATE TABLE posts (id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, discussion_id INTEGER NOT NULL, number INTEGER DEFAULT NULL, created_at DATETIME NOT NULL, user_id INTEGER DEFAULT NULL, type VARCHAR(255) DEFAULT NULL, content CLOB DEFAULT NULL --
, edited_at DATETIME DEFAULT NULL, edited_user_id INTEGER DEFAULT NULL, hidden_at DATETIME DEFAULT NULL, hidden_user_id INTEGER DEFAULT NULL, ip_address VARCHAR(255) DEFAULT NULL, is_private BOOLEAN DEFAULT 0 NOT NULL, "is_approved" tinyint(1) not null default '1', foreign key("discussion_id") references "discussions"("id") on delete cascade, foreign key("user_id") references "users"("id") on delete set null, foreign key("edited_user_id") references "users"("id") on delete set null, foreign key("hidden_user_id") references "users"("id") on delete set null);
CREATE INDEX posts_user_id_created_at_index ON posts (user_id, created_at);
CREATE INDEX posts_discussion_id_created_at_index ON posts (discussion_id, created_at);
CREATE INDEX posts_discussion_id_number_index ON posts (discussion_id, number);
CREATE UNIQUE INDEX posts_discussion_id_number_unique ON posts (discussion_id, number);
CREATE INDEX "posts_type_index" on "posts" ("type");
CREATE INDEX "posts_type_created_at_index" on "posts" ("type", "created_at");
CREATE TABLE IF NOT EXISTS "unsubscribe_tokens" ("id" integer primary key autoincrement not null, "user_id" integer not null, "email_type" varchar not null, "token" varchar not null, "unsubscribed_at" datetime, "created_at" datetime, "updated_at" datetime, foreign key("user_id") references "users"("id") on delete cascade);
CREATE INDEX "unsubscribe_tokens_user_id_index" on "unsubscribe_tokens" ("user_id");
CREATE INDEX "unsubscribe_tokens_email_type_index" on "unsubscribe_tokens" ("email_type");
CREATE INDEX "unsubscribe_tokens_token_index" on "unsubscribe_tokens" ("token");
CREATE INDEX "unsubscribe_tokens_user_id_email_type_index" on "unsubscribe_tokens" ("user_id", "email_type");
CREATE UNIQUE INDEX "unsubscribe_tokens_token_unique" on "unsubscribe_tokens" ("token");
CREATE TABLE discussions (id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, title VARCHAR(255) NOT NULL COLLATE "BINARY", comment_count INTEGER DEFAULT 1 NOT NULL, participant_count INTEGER DEFAULT 0 NOT NULL, created_at DATETIME NOT NULL, user_id INTEGER DEFAULT NULL, first_post_id INTEGER DEFAULT NULL, last_posted_at DATETIME DEFAULT NULL, last_posted_user_id INTEGER DEFAULT NULL, last_post_id INTEGER DEFAULT NULL, last_post_number INTEGER DEFAULT NULL, hidden_at DATETIME DEFAULT NULL, hidden_user_id INTEGER DEFAULT NULL, slug VARCHAR(255) NOT NULL COLLATE "BINARY", is_private BOOLEAN DEFAULT 0 NOT NULL, "is_approved" tinyint(1) not null default '1', "is_sticky" tinyint(1) not null default '0', "is_locked" tinyint(1) not null default '0', foreign key("first_post_id") references "posts"("id") on delete set null, foreign key("last_post_id") references "posts"("id") on delete set null, foreign key("user_id") references "users"("id") on delete set null, foreign key("last_posted_user_id") references "users"("id") on delete set null, foreign key("hidden_user_id") references "users"("id") on delete set null);
CREATE INDEX discussions_last_posted_at_index ON discussions (last_posted_at);
CREATE INDEX discussions_last_posted_user_id_index ON discussions (last_posted_user_id);
CREATE INDEX discussions_created_at_index ON discussions (created_at);
CREATE INDEX discussions_user_id_index ON discussions (user_id);
CREATE INDEX discussions_comment_count_index ON discussions (comment_count);
CREATE INDEX discussions_participant_count_index ON discussions (participant_count);
CREATE INDEX discussions_hidden_at_index ON discussions (hidden_at);
CREATE TABLE flags (id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, post_id INTEGER NOT NULL, type VARCHAR(255) NOT NULL, user_id INTEGER DEFAULT NULL, reason VARCHAR(255) DEFAULT NULL, reason_detail CLOB DEFAULT NULL, created_at DATETIME NOT NULL);
CREATE INDEX flags_created_at_index ON flags (created_at);
CREATE TABLE IF NOT EXISTS "discussion_tag" ("discussion_id" integer not null, "tag_id" integer not null, "created_at" datetime, primary key ("discussion_id", "tag_id"));
CREATE TABLE tags (id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, name VARCHAR(255) NOT NULL, slug VARCHAR(100) NOT NULL, description CLOB DEFAULT NULL, color VARCHAR(255) DEFAULT NULL, background_path VARCHAR(255) DEFAULT NULL, background_mode VARCHAR(255) DEFAULT NULL, position INTEGER DEFAULT NULL, parent_id INTEGER UNSIGNED DEFAULT NULL, default_sort VARCHAR(255) DEFAULT NULL, is_restricted BOOLEAN DEFAULT 0 NOT NULL, is_hidden BOOLEAN DEFAULT 0 NOT NULL, discussion_count INTEGER DEFAULT 0 NOT NULL, last_posted_at DATETIME DEFAULT NULL, last_posted_discussion_id INTEGER DEFAULT NULL, "last_posted_user_id" integer, "icon" varchar, "created_at" datetime, "updated_at" datetime);
CREATE UNIQUE INDEX tags_slug_unique ON tags (slug);
CREATE TABLE tag_user (user_id INTEGER NOT NULL, tag_id INTEGER NOT NULL, marked_as_read_at DATETIME DEFAULT NULL, is_hidden BOOLEAN DEFAULT 0 NOT NULL, PRIMARY KEY(user_id, tag_id));
CREATE TABLE IF NOT EXISTS "post_mentions_tag" ("post_id" integer not null, "mentions_tag_id" integer not null, "created_at" datetime default CURRENT_TIMESTAMP, foreign key("post_id") references "posts"("id") on delete cascade, foreign key("mentions_tag_id") references "tags"("id") on delete cascade, primary key ("post_id", "mentions_tag_id"));
CREATE TABLE users (id INTEGER PRIMARY KEY AUTOINCREMENT NOT NULL, username VARCHAR(255) NOT NULL COLLATE "BINARY", email VARCHAR(255) NOT NULL COLLATE "BINARY", is_email_confirmed BOOLEAN DEFAULT 0 NOT NULL, password VARCHAR(255) NOT NULL COLLATE "BINARY", avatar_url VARCHAR(255) DEFAULT NULL COLLATE "BINARY", preferences BLOB DEFAULT NULL, joined_at DATETIME DEFAULT NULL, last_seen_at DATETIME DEFAULT NULL, marked_all_as_read_at DATETIME DEFAULT NULL, read_notifications_at DATETIME DEFAULT NULL, discussion_count INTEGER DEFAULT 0 NOT NULL, comment_count INTEGER DEFAULT 0 NOT NULL, read_flags_at DATETIME DEFAULT NULL, suspended_until DATETIME DEFAULT NULL, "suspend_reason" text, "suspend_message" text);
CREATE INDEX users_comment_count_index ON users (comment_count);
CREATE INDEX users_discussion_count_index ON users (discussion_count);
CREATE INDEX users_last_seen_at_index ON users (last_seen_at);
CREATE INDEX users_joined_at_index ON users (joined_at);
CREATE UNIQUE INDEX users_username_unique ON users (username);
CREATE UNIQUE INDEX users_email_unique ON users (email);
CREATE INDEX "discussions_is_sticky_created_at_index" on "discussions" ("is_sticky", "created_at");
CREATE INDEX "discussions_is_sticky_last_posted_at_index" on "discussions" ("is_sticky", "last_posted_at");
CREATE TABLE post_mentions_post (post_id INTEGER NOT NULL, mentions_post_id INTEGER NOT NULL, "created_at" datetime, PRIMARY KEY(post_id, mentions_post_id));
CREATE TABLE post_mentions_user (post_id INTEGER NOT NULL, mentions_user_id INTEGER NOT NULL, "created_at" datetime, PRIMARY KEY(post_id, mentions_user_id));
CREATE TABLE IF NOT EXISTS "post_mentions_group" ("post_id" integer not null, "mentions_group_id" integer not null, "created_at" datetime default CURRENT_TIMESTAMP, foreign key("post_id") references "posts"("id") on delete cascade, foreign key("mentions_group_id") references "groups"("id") on delete cascade, primary key ("post_id", "mentions_group_id"));
CREATE INDEX "discussions_is_locked_index" on "discussions" ("is_locked");
CREATE TABLE IF NOT EXISTS "post_likes" ("post_id" integer not null, "user_id" integer not null, "created_at" datetime not null default CURRENT_TIMESTAMP, primary key ("post_id", "user_id"));
